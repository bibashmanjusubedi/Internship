@model Internship.Models.AssetOut

<h2>Delete Asset Out</h2>

<h4>Are you sure you want to delete this?</h4>
<div>
    <dl class="row">
        <dt class="col-sm-2">Asset Code</dt>
        <dd class="col-sm-10">@Model.AssetCode</dd>

        <dt class="col-sm-2">Assigned To (PId)</dt>
        <dd class="col-sm-10">@Model.PId</dd>

        <dt class="col-sm-2">Out Date</dt>
        <dd class="col-sm-10">@Model.OutDate</dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Sn" />
        <button type="submit" class="btn btn-danger">Delete</button> |
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </form>
</div>

