@model Internship.Models.AssetOut

@{
    ViewData["Title"] = "Create New Asset Out";
}

<h2>Create New Asset Out</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="Sn" class="control-label"></label>
        <input asp-for="Sn" class="form-control" />
        <span asp-validation-for="Sn" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="AssetCode" class="control-label"></label>
        <input asp-for="AssetCode" class="form-control" />
        <span asp-validation-for="AssetCode" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="OutDate" class="control-label"></label>
        <input asp-for="OutDate" class="form-control" />
        <span asp-validation-for="OutDate" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="PId" class="control-label"></label>
        <input asp-for="PId" class="form-control" />
        <span asp-validation-for="PId" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="DateToReturn" class="control-label"></label>
        <textarea asp-for="DateToReturn" class="form-control"></textarea>
        <span asp-validation-for="DateToReturn" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="ReturnDate" class="control-label"></label>
        <input asp-for="ReturnDate" class="form-control" />
        <span asp-validation-for="ReturnDate" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Remarks" class="control-label"></label>
        <input asp-for="Remarks" class="form-control" />
        <span asp-validation-for="Remarks" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Create Asset Out</button>
    <a asp-action="Index" class="btn btn-secondary">Back to Asset Out List</a>
</form>

